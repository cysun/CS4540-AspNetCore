@model List<Employee>

<table class="table">
    <thead>
        <tr><th>Id</th><th>Name</th><th>Date Hired</th><th>Supervisor</th></tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.DateHired</td>
                <td>
                    @if (employee.Supervisor != null)
                    {
                        @employee.Supervisor.Name
                    }
                </td>
            </tr>
        }
    </tbody>
</table>